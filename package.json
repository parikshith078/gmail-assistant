{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gmail-client",
  "title": "Gmail_client",
  "description": "Manage inbox right here in raycast",
  "icon": "command-icon.png",
  "author": "Parikshith",
  "categories": [
    "Productivity",
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Send email",
      "subtitle": "Send email to anyone",
      "description": "Send email to anyone",
      "mode": "view"
    },
    {
      "name": "checkInbox",
      "title": "Check inbox",
      "subtitle": "Shows your upto date inbox",
      "description": "Shows your upto date inbox",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.3",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
